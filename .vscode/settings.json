{
	"latex-workshop.latex.bibDirs": [
		"./bib"
	],
	"latex-workshop.latex.texDirs": [
		"./src",
		"./include"
	],
	"latex-workshop.latex.search.rootFiles.include": [
		"**/*.tex",
		"*.tex"
	],
	"latex-workshop.latex.outDir": "%DIR%/out/",
	"latex-workshop.chktex.run": "onType",
	"latex-workshop.chktex.args.root": [
		"-wall",
		"-n22",
		"-n30",
		"-e16",
		"-q",
		"%DIR%"
	],
	"latex-workshop.synctex.afterBuild.enabled": true,
	"terminal.integrated.shell.windows": "cmd.exe",
	"latex-workshop.latex.autoBuild.run": "onFileChange",
	"latex-workshop.latex.autoBuild.cleanAndRetry.enabled": true,
	"cSpell.language": ",ru,en-GB,en",
	"search.usePCRE2": true,
	"editor.rulers": [
		80,
		120
	],
	"latex-workshop.latex.option.maxPrintLine.enabled": false
}